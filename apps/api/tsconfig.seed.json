{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "moduleResolution": "node",
      "outDir": "prisma/build",
      "rootDir": "prisma",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "noEmit": false,
      "allowImportingTsExtensions": false,
      "verbatimModuleSyntax": false
    },
    "include": ["prisma/seed.ts"]
  }
  